DJANGO_SETTINGS_MODULE=config.settings.local
DJANGO_SECRET_KEY=change-me-for-local
DJANGO_DEBUG=true
DJANGO_ALLOWED_HOSTS=127.0.0.1,localhost
PUBLIC_BACKEND_ORIGIN=

DB_ENGINE=sqlite3
DB_NAME=db.sqlite3

CORS_ALLOWED_ORIGINS=http://127.0.0.1:5173,http://localhost:5173,http://127.0.0.1:5174,http://localhost:5174
CSRF_TRUSTED_ORIGINS=http://127.0.0.1:5173,http://localhost:5173,http://127.0.0.1:5174,http://localhost:5174

USE_REDIS_CACHE=false
REDIS_CACHE_URL=redis://127.0.0.1:6379/1
REDIS_SESSION_URL=redis://127.0.0.1:6379/2
CACHE_DEFAULT_TTL=300
SESSION_CACHE_TTL=1209600
ALLOW_REMOTE_REDIS_IN_LOCAL=false

USE_S3_MEDIA=false
PRESIGNED_UPLOAD_EXPIRES_IN=300
PRESIGNED_UPLOAD_MAX_BYTES=10485760
PRESIGNED_UPLOAD_PREFIX=uploads
PRESIGNED_ALLOWED_CONTENT_PREFIXES=image/
ALLOW_S3_MEDIA_IN_LOCAL=false
ALLOW_REMOTE_DB_IN_LOCAL=false

KAKAO_REST_API_KEY=
KAKAO_CLIENT_SECRET=
KAKAO_REDIRECT_URI=http://localhost:5173/pages/kakao-callback.html
KAKAO_ALLOWED_REDIRECT_URIS=http://localhost:5173/pages/kakao-callback.html,http://localhost:5174/pages/kakao-callback.html
